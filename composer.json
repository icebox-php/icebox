{
    "name": "icebox-php/icebox",
    "type": "project",
    "description": "The skeleton application for the Icebox Framework.",
    "keywords": ["framework", "icebox"],
    "license": "MIT",
    "homepage": "http://torovin.com/icebox",
    "require": {
      "icebox-php/framework": "dev-main",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
          "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
